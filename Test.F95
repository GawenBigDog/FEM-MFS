program Test
    Use INTERPRETER
    Use FILEM
    USE INPUTM
    USE GENERATEM
    USE SOLVERM
    USE OUTPUTM
    USE ODEM
    IMPLICIT NONE
!
    INTEGER :: I
    INTEGER :: IIN,IOUT,ADELE,ADK,ADM
    INTEGER :: ADDISP,ADSTRESS
!
    IIN=InputAD()
    CALL OPENFILES
!   READ INPUT FILE
    Call ReadFile(IIN)
!--- START
    IOUT=OutputAD()
    ADELE=OutputEle()
    CALL INPUT(IIN,IOUT,ADELE)
!
    ADK=OutputK()
    ADM=OutputMa()
    CALL GENERATE(ADK,ADM,ADELE)
!
    CALL COLSOL_PRE()
    CALL COLSOL(K_COLSOL,DISP,MAXA_COLSOL,NEQ,NWK_COLSOL,NEQ+1,1)
    CALL COLSOL(K_COLSOL,DISP,MAXA_COLSOL,NEQ,NWK_COLSOL,NEQ+1,2)
!
    ADDISP    =OutputDisp()
    ADSTRESS  =OutputStress()
    CALL OUTPUT(ADDISP,ADSTRESS,ADELE)
!
    CALL CLOSEFILES
    CALL RELEASE
end program Test
